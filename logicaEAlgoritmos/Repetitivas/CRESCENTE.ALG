Algoritmo "crescente"



Var

   x,y : inteiro
   
Inicio

      escreva("Digite dois números: ")
      leia(x, y)
      
      enquanto x <> y faca
      
      se x > y entao
         escreval("Decrescente")
      senao
           escreval("Crescente")
      fimse

      escreval("Digite outros dois números: ")
      leia(x, y)

      fimenquanto

Fimalgoritmo